------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac
Content-Type: text/ecl
Entry-Name: .content

get-view "Project Explorer" | get-tree | get-menu "New/Capella Project" | click
with [get-window "New Capella Project"] {
    with [get-editbox] {
        set-text DiagramViewerTest
    }
    get-button Finish| click
}
with [get-editor DiagramViewerTest] {
    get-tab-folder | get-tab-item "Operational Analysis" | click
    with [get-section "Define Operational Entities and Capabilities"] {
        click
        get-link "[OEBD] Create a new Operational Entity Breakdown diagram" | click
    }
}
get-window "New Operational Entity Breakdown" | get-button OK | click
get-editor "[OEBD] Operational Entities" | get-diagram -index 1 | get-edit-part -name "[OEBD] Operational Entities"

// Save the model
get-eclipse-window | key-type "M1+s"

with [get-editor DiagramViewerTest] {
    click
    with [get-section "Diagrams Viewer" | get-tree] {
        select "Operational Analysis/Operational Entity Breakdown/[OEBD] Operational Entities" | get-menu 
            -path "Show in Project Explorer" | click
        select "Operational Analysis/Operational Entity Breakdown/[OEBD] Operational Entities" | get-menu -path Open 
            | click
    }
}
get-editor "[OEBD] Operational Entities" | get-diagram -index 1 | get-edit-part -name "[OEBD] Operational Entities" 
    | mouse-move 54 3 -height 608 -width 1286
with [get-editor DiagramViewerTest] {
    click
    with [get-section "Diagrams Viewer" | get-tree] {
        select "Operational Analysis/Operational Entity Breakdown/[OEBD] Operational Entities" | get-menu 
            -path "Clone Diagram" | click
        select "Operational Analysis/Operational Entity Breakdown/Clone of [OEBD] Operational Entities" | get-menu 
            -path Rename | click
    }
}
with [get-window "Rename representation"] {
    get-editbox -after [get-label "Enter new name (case insensitive):"] | set-text Bob
    get-button OK | click
}
get-editor DiagramViewerTest | get-section "Diagrams Viewer" | get-tree 
    | select "Operational Analysis/Operational Entity Breakdown/Bob" | get-menu -path Delete | click
get-window "Confirm Delete" | get-button OK | click
------=_.content-0a7243a0-75d3-3d5f-9791-539de0e5b7ac--
